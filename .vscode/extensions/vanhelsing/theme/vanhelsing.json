            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Generic selectors (CSS/SCSS/Less/Stylus)",
            "scope": [
                "meta.selector"
            ],
            "settings": {
                "foreground": "#ff4d91"
            }
        },
        {
            "name": "Language Built-ins",
            "scope": [
                "support"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#96d4d8"
            }
        },
        {
            "name": "Built-in magic functions and constants",
